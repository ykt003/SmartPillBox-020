package com.rilintech.unit;

public class GlobalVar {
	public static int getAndroidSDKVersion() {
		int version = 0;
		try {
		version = Integer.valueOf(android.os.Build.VERSION.SDK);
		} catch (NumberFormatException e) {
		//CSLog.e(Tool.class, e.toString());
		}
		return version;
		}
}
